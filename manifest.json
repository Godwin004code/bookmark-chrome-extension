{
    "manifest_version": 3,
    "name": "Custom Bookmarks Tab",
    "version": "1.0",
    "description": "A Chrome extension to replace the bookmarks tab.",
    "permissions": ["bookmarks", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon48.png"
      }
    },
    "icons": {
      "16": "icons/icon48.png"
    }
  }
  